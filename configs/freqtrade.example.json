{
    "// NOTE": "This is a SKELETON config - copy to freqtrade.json and fill locally",
    "// SECURITY": "NO API KEYS in this file - placeholders only!",
    "max_open_trades": 3,
    "stake_currency": "USDT",
    "stake_amount": 100,
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 1000,
    "// EXCHANGE": "Placeholder - does NOT perform live trading",
    "exchange": {
        "name": "binance",
        "// API_KEYS": "FILL LOCALLY! Never push secrets to Git",
        "key": "YOUR_API_KEY_HERE",
        "secret": "YOUR_SECRET_HERE",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "// MARKETS": "Refresh interval pro market data",
        "markets_refresh_interval": 60
    },
    "// PAIRLIST": "STATIC for reproducible backtests - important",
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],
    "// PAIRS": "Defined static pairlist for consistent results",
    "pair_whitelist": [
        "BTC/USDT",
        "ETH/USDT",
        "ADA/USDT",
        "DOT/USDT"
    ],
    "// TIMEFRAMES": "For the MATRIX pipeline",
    "timeframe": "5m",
    "// STRATEGY": "Placeholder for MATRIX strategy",
    "strategy": "MatrixStrategy",
    "strategy_path": "strategies/",
    "// BACKTEST": "Settings for walk-forward validation",
    "timerange": "20241101-20241201",
    "enable_protections": false,
    "// TELEGRAM": "Notifications - fill locally",
    "telegram": {
        "enabled": false,
        "token": "YOUR_TELEGRAM_TOKEN",
        "chat_id": "YOUR_CHAT_ID"
    },
    "// PATHS": "Data and log paths",
    "datadir": "user_data/data",
    "user_data_dir": "user_data",
    "// LOGS": "Debugging for MATRIX",
    "verbosity": 3,
    "logfile": "logs/freqtrade.log",
    "// API": "Webserver for monitoring",
    "api_server": {
        "enabled": false,
        "listen_ip_address": "127.0.0.1",
        "listen_port": 8080,
        "username": "freqtrade",
        "password": "freqtrade_password"
    }
}